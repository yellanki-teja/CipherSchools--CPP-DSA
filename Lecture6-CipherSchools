#include <iostream>
using namespace std;
int main() {
   
    
    int a;
    cout<<"Enter an integer a: ";
    cin>>a;
    
    switch (a){
        case 1:
            cout<<"hehe";
            break;
        case 2:
            cout<<"haha";
            break;
        default:
            cout<<"Default will be always printed";
            break;
    }
    
}
