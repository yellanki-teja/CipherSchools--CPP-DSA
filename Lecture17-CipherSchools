#include <iostream>
using namespace std;

void PrintHi(){
    cout<<"Hi"<<endl;
    return;
}
int sum(int a,int b){
    int c;
    c=a+b;
    return c;
}
int main() {
    PrintHi();
    
    int a,b;
    a=3;
    b=5;
    int d;
    d= sum(a,b);
    cout<<"The sum is equal to "<<d<<endl;
    return 0;
    
}
