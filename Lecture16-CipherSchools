#include <iostream>
using namespace std;

int sum(int a, int b); 

int main() {
    int a,b;
    
    a=3;
    b=5;
    cout<<a<<" "<<b<<endl;   // the values of a,b before I call the function "sum"
    
    int c;
    c=sum(a,b);
    
    cout<<c<<endl;
     cout<<a<<" "<<b<<endl;  // the values of a,b after I call the function "sum"
    return 0;
}

int sum(int a, int b){
    int c;
    c=a+b;
    
    a =10;
    b = 11;
    
    return c;
}
